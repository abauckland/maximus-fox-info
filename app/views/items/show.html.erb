<div class="row title_band">    <div class="page_title">        <h1>Item Details</h1>    </div></div><div class="row">    <div class="col-1-3 column_1">        <div class="border">            <div class="row">            <form action="<%= keynote_export_keynote_path %>" method="get" >            	<%= hidden_field_tag "id", @project.id %>                    <fieldset><table>    <% @parents.each do |parent| %>    <tr>      <% parent.itemitems.each do  |item| %>        <td><%= item.item %></td>      <% end %>    </tr>    <% end %></table>                </fieldset>            </div>			<!-- column footer start-->             <div class="row">                <div class="form_footer">                    <%= submit_tag "download"%>                </div>                </form>            </div>        </div>    </div>    <div class="col-1-3 column_2">        <div class="border blank_column"></div>    </div>    <div class="col-1-3 column_3">        <div class="border blank_column"></div>    </div></div>